<?xml version="1.0"?>
<info xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://apps.nextcloud.com/schema/apps/info.xsd">
    <id>corecomdashboard</id>
    <name>Corecom Dashboard</name>
    <summary>This application allows you to reproduce videos from a nextcloud folder.</summary>
    <description>
        <![CDATA[This application allows you to reproduce videos inside a playlist from a nextcloud folder.]]>
    </description>
    <version>0.0.3</version>
    <licence>agpl</licence>
    <author mail="garofalomarco58@gmail.com">Marco Garofalo</author>
    <namespace>CorecomDashboard</namespace>
    <category>multimedia</category>
    <bugs>https://issueTracker.com</bugs>
    <dependencies>
        <nextcloud min-version="19" max-version="20" />
    </dependencies>

    <settings>
        <personal>OCA\CorecomDashboard\Settings\PersonalSettings</personal>
        <personal-section>OCA\CorecomDashboard\Settings\PersonalSection</personal-section>
        <admin>OCA\CorecomDashboard\Settings\AdminSettings</admin>
        <admin-section>OCA\CorecomDashboard\Settings\AdminSection</admin-section>
    </settings>

    <navigations>
        <navigation>
            <name>CorecomDashboard</name>
            <route>corecomdashboard.page.index</route>
        </navigation>
    </navigations>
</info>
